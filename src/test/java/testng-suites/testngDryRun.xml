<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite thread-count="1" name="ENDCLOTHING-AUTOMATION" parallel="tests"
	preserve-order="true" verbose="1">
	<listeners>
		<listener class-name="endclothing.qa.base.TestListener" />
		<listener class-name="endclothing.qa.base.AnnotationTransformer" />
	</listeners>

	<test thread-count="1" name="Chrome_Suite" parallel="methods">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="endclothing.qa.test.PocTest"></class>
		</classes>
	</test>

</suite>